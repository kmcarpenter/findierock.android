<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
   	<LinearLayout android:orientation="vertical" android:id="@+id/linearLayout1" android:layout_height="fill_parent" android:layout_width="fill_parent">
	<RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="match_parent" android:layout_height="wrap_content" android:background="@color/bg_color" android:padding="4dip">
		<TextView android:text="TextView" android:layout_width="wrap_content" android:id="@+id/prevView" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentLeft="true" android:textColor="@color/bg_color_header" android:textStyle="bold"></TextView>
		<TextView android:text="TextView" android:layout_width="wrap_content" android:id="@+id/thisView" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:textColor="@color/bg_color_header" android:textStyle="bold"></TextView>
		<TextView android:text="TextView" android:layout_width="wrap_content" android:id="@+id/nextView" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:textColor="@color/bg_color_header" android:textStyle="bold"></TextView>
	</RelativeLayout>
	<LinearLayout android:orientation="vertical" android:id="@+id/event_progress" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_gravity="center_vertical">
		<ProgressBar android:id="@+id/event_progress_real" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_gravity="center"></ProgressBar>
		<TextView android:text="Loading Events..." android:layout_width="wrap_content" android:id="@+id/eventlist_loading" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:textStyle="bold" android:layout_gravity="center"></TextView>
	</LinearLayout>
	<TextView android:text="No Events..." android:layout_width="wrap_content" android:id="@+id/eventlist_noevents" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:textStyle="bold" android:layout_gravity="center" android:visibility="gone"></TextView>
	<ListView android:layout_width="wrap_content"
       android:id="@+id/eventlist_list" android:cacheColorHint="@color/bg_color_dev" android:layout_height="fill_parent"/>
	</LinearLayout>
</LinearLayout>
